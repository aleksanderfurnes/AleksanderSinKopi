definition
	EVALUATION[at0000] matches {    -- helseopplysninger_antibiotikaresistente_bakterier_hn
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0012] occurrences matches {0..1} matches {    -- Reise utenfor Norden
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0027] occurrences matches {0..1} matches {    -- Har du i løpet av de siste 12 måneder vært i et land utenfor Norden, hvor du har:
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0028,    -- Vært innlagt i helseinstitusjon?
											at0029,    -- Fått omfattende undersøkelse eller behandling i en helsetjeneste eller hos tannlege?
											at0030,    -- Arbeidet som helsearbeider?
											at0031,    -- Oppholdt deg i barnehjem eller flyktningsleir?
											at0032]    -- Nei, ingen av valgene over gjelder meg.
										}
									}
								}
							}
							ELEMENT[at0023] occurrences matches {0..1} matches {    -- Har du etter hjemkomst tatt en MRSA-prøve?
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0024,    -- Ja, den var negativ
											at0025,    -- Ja, den var positiv
											at0026]    -- Nei jeg har ikke tatt MRSA-prøve
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0013] matches {*}    -- Item tree
		}
	}

