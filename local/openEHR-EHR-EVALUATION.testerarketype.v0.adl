definition
	EVALUATION[at0000] matches {    -- testerarketype
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Fritekst
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Kodet tekst
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0004,    -- Valg1
									at0005,    -- Valg2
									at0006,    -- Valg3
									at0007,    -- Valg4
									at0008]    -- Valg5
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Kvantitet
						value matches {
							DV_QUANTITY matches {*}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Proporsjon
						value matches {
							DV_PROPORTION matches {*}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- Heltall
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- DatoTid
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Tid
						value matches {
							DV_TIME matches {*}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Dato
						value matches {
							DV_DATE matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Ordinal
						value matches {
							1|[local::at0016],
							2|[local::at0017],
							3|[local::at0018],
							4|[local::at0019]
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {    -- Boolean1
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {    -- Boolean2
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {    -- Boolean3
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					allow_archetype CLUSTER[at0023] occurrences matches {0..*} matches {    -- CLUSTER_SLOT
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

